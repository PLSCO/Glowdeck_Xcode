    else if (type == F("twitter")) pxs.setColor(0, 173, 238);     // TWITTER (LIGHT BLUE)
    else if (type == F("email")) pxs.setColor(105, 188, 69);	  // EMAIL (GREEN)
    else if (type == F("news")) pxs.setColor(250, 175, 58);	  // NEWS (ORANGE)
    
    if (q > 7) q = 7;
    else if (q < 1) q = 1;
    if (id > q) id = q;
    else if (id < 0) id = 0;
    
    float wf = (float)((totalWidth / q) + 1.0);
    
    int w = (int)wf;
    
    if (streamSegmentWidth != w) streamSegmentWidth = w;
    
    int x = 0 + (w * id);
    int y = 174; int h = 17;
    
    pxs.fillRectangle(x, y, w, h);
    
}

