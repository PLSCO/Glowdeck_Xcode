void printStreamSpectrum(String active, int q) {
    
    int key = 1;
    
    for (int i = 0; i < 7; i++) {
        
        if (streamTypes[i] == active) {
            
            key = i;
            
            break;
            
        }
        
    }
    
    for (int i = 0; i < q; i++) {
        
        printStreamSquare(streamTypes[key], i, q);
        
        key = (key + 1) % 7;
        
    }
    
}

void printStreamSquare(String type, int id, int q) {
    
    if (type == F("weather")) pxs.setColor(252, 238, 33);	  // WEATHER (YELLOW)
    else if (type == F("instagram")) pxs.setColor(250, 175, 58);  // INSTAGRAM (ORANGE)
