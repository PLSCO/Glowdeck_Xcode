void setup() {
    
    Serial.begin(9600);
    
    ble.begin(9600);
    
    Serial.println(F("Glowdeck starting up..."));
    
    Serial.println(F(""));
    
    init_adc();
    
    init_audio();
    
    init_bluetooth();
    
    init_leds();
    
    init_lcd();
    
    Serial.println(F("Init success!"));
    
    Serial.println(F(""));
    
    runStreamsDemo();
    
}

int loopCount = 0;

void loop() {
    
    runStreamsDemo();
    
    loopCount++;
    
    Serial.print(F("RunLoop: "));
    
    Serial.println(String(loopCount));
    
    //serialLoop();
    
}

void serialLoop() {
    
    if (Serial.available()) {
        
        while (Serial.available()) {
            
            char inChar = Serial.read();
            
            ble.print(inChar);
            
        }
        
    }
    
    if (ble.available()) {
        
        while (ble.available()) {
            
            char inChar = ble.read();
            
            Serial.print(inChar);
            
        }
        
    }
    
}



